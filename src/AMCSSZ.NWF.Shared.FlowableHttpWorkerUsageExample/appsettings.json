{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System.Net.Http": "Warning"
    }
  },
  "FlowableRest": {
    "Client": {
      "BaseUrl": "http://flowable-rest:8080/flowable-rest/external-job-api",
      "User": "rest-admin",
      "Pass": "test",
      "AllowInsecureSsl": true,
      "HttpTimeoutSeconds": 30
    }
  },
  "HttpExternalTaskHandler": {
    "Flowable": {
      "Topic": "srd.call",
      "WorkerId": "flowable-http-worker-usage-example-1",
      "TenantId": "tenant-a",
      "LockDuration": "PT30S",
      "MaxJobsPerTick": 10,
      "PollPeriodSeconds": 2,
      "MaxDegreeOfParallelism": 4,
      "InitialRetries": 3,
      "Retry": {
        "InitialDelaySeconds": 60,
        "MaxDelaySeconds": 900,
        "JitterSeconds": 5
      },
      "TimeWindow": {
        "TimeZoneId": "Europe/Prague",
        "DailySchedules": {
          "Monday": {
            "ActiveWindows": [
              {
                "Start": "08:00:00",
                "End": "24:00:00"
              }
            ]
          },
          "Tuesday": {
            "ActiveWindows": [
              {
                "Start": "00:00:00",
                "End": "24:00:00"
              }
            ]
          },
          "Wednesday": {
            "ActiveWindows": [
              {
                "Start": "00:00:00",
                "End": "15:27:00"
              }
            ]
          },
          "Thursday": {
            "ActiveWindows": [
              {
                "Start": "00:00:00",
                "End": "24:00:00"
              }
            ]
          },
          "Friday": {
            "ActiveWindows": [
              {
                "Start": "00:00:00",
                "End": "16:00:00"
              }
            ]
          },
          "Saturday": {
            "Enabled": false
          },
          "Sunday": {
            "Enabled": false
          }
        }
      }
    },
    "Http": {
      "TargetUrl": "http://flowable-http-worker-usage-example:5005/api/events",
      "TimeoutSeconds": 10
    }
  },
  "HttpExternalTaskHandler2": {
    "Flowable": {
      "Topic": "srd2.call",
      "WorkerId": "flowable-http-worker-usage-example-2",
      "TenantId": null,
      "LockDuration": "PT30S",
      "MaxJobsPerTick": 10,
      "PollPeriodSeconds": 2,
      "MaxDegreeOfParallelism": 4,
      "InitialRetries": 3,
      "Retry": {
        "InitialDelaySeconds": 30,
        "MaxDelaySeconds": 300,
        "JitterSeconds": 5,
        "BackoffMultiplier": 1.5
      }
    },
    "Http": {
      "TargetUrl": "http://flowable-http-worker-usage-example:5005/api/events",
      "TimeoutSeconds": 10
    }
  }
}
