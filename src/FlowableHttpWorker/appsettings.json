{
  "FlowableRest": {
    "Client": {
      "BaseUrl": "http://flowable-rest:8080/flowable-rest/external-job-api",
      "User": "rest-admin",
      "Pass": "test",
      "AllowInsecureSsl": true,
      "HttpTimeoutSeconds": 30
    }
  },
  "FlowableWorkers": {
    "HttpExternalTaskHandler": {
      "Flowable": {
        "Topic": "srd.call",
        "WorkerId": "flowable-http-worker-1",
        "LockDuration": "PT30S",
        "MaxJobsPerTick": 10,
        "PollPeriodSeconds": 2,
        "MaxDegreeOfParallelism": 4,
        "InitialRetries": 3,
        "Retry": {
          "InitialDelaySeconds": 60,
          "MaxDelaySeconds": 900,
          "JitterSeconds": 5
        },
        "TimeWindow": {
          "TimeZoneId": "Europe/Prague",
          "PauseFromHour": 14,
          "PauseToHourExclusive": 15
        }
      },
      "Http": {
        "TargetUrl": "http://flowable-http-worker:5005/api/events",
        "TimeoutSeconds": 10
      }
    },
    "HttpExternalTaskHandler2": {
      "Flowable": {
        "Topic": "srd2.call",
        "WorkerId": "flowable-http-worker-2",
        "LockDuration": "PT30S",
        "MaxJobsPerTick": 10,
        "PollPeriodSeconds": 2,
        "MaxDegreeOfParallelism": 4,
        "InitialRetries": 3,
        "Retry": {
          "InitialDelaySeconds": 30,
          "MaxDelaySeconds": 300,
          "JitterSeconds": 5,
          "BackoffMultiplier": 1.5
        }
      },
      "Http": {
        "TargetUrl": "http://flowable-http-worker:5005/api/events",
        "TimeoutSeconds": 10
      }
    }
  }
}
